{
    "name": "SiteCleaner",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "js": [
            "contentscript.js",
            "background.js"
        ],
        "matches": ["<all_urls>"],
        "run_at": "document_end"
    }],
    "web_accessible_resources": [
        "weibo.js",
        "baidu.js",
        "svg.js",
        "svg-lib.js",
        "jquery.js"
    ],
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}